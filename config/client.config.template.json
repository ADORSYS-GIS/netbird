{
  "ManagementURL": "{{ NETBIRD_MANAGEMENT_URL | default('https://api.netbird.io:443') }}",
  "AdminURL": "{{ NETBIRD_ADMIN_URL | default('https://app.netbird.io') }}",
  "ConfigPath": "{{ NETBIRD_CONFIG_PATH | default('/etc/netbird/config.json') }}",
  "LogFile": "{{ NETBIRD_LOG_FILE | default('/var/log/netbird/client.log') }}",
  "LogLevel": "{{ NETBIRD_LOG_LEVEL | default('info') }}",
  "SetupKey": "{{ NETBIRD_SETUP_KEY | required }}",
  "PreSharedKey": "{{ NETBIRD_PRESHARED_KEY | default('') }}",
  "Interface": {
    "Name": "{{ NETBIRD_INTERFACE_NAME | default('wt0') }}",
    "Address": "{{ NETBIRD_INTERFACE_ADDRESS | default('100.64.0.0/10') }}",
    "MTU": {{ NETBIRD_INTERFACE_MTU | default(1280) }},
    "ListenPort": {{ NETBIRD_LISTEN_PORT | default(51820) }}
  },
  "WireguardInterface": {
    "Name": "{{ NETBIRD_WG_INTERFACE_NAME | default('wt0') }}",
    "Address": "{{ NETBIRD_WG_INTERFACE_ADDRESS | default('100.64.0.0/10') }}",
    "MTU": {{ NETBIRD_WG_INTERFACE_MTU | default(1280) }},
    "ListenPort": {{ NETBIRD_WG_LISTEN_PORT | default(51820) }}
  },
  "IFaceBlackList": {{ NETBIRD_IFACE_BLACKLIST | default('["tun", "utun", "tap", "tailscale"]') }},
  "DisableAutoConnect": {{ NETBIRD_DISABLE_AUTO_CONNECT | default(false) }},
  "ServerSSHAllowed": {{ NETBIRD_SERVER_SSH_ALLOWED | default(false) }},
  "RosenpassEnabled": {{ NETBIRD_ROSENPASS_ENABLED | default(false) }},
  "RosenpassPermissive": {{ NETBIRD_ROSENPASS_PERMISSIVE | default(false) }},
  "InterfaceBlackList": {{ NETBIRD_INTERFACE_BLACKLIST | default('[]') }},
  "FirewallMark": {{ NETBIRD_FIREWALL_MARK | default(0) }},
  "DNS": {
    "NameServers": {{ NETBIRD_DNS_NAMESERVERS | default('["100.64.0.100"]') }},
    "Domains": {{ NETBIRD_DNS_DOMAINS | default('["netbird.local"]') }},
    "CustomZones": {{ NETBIRD_DNS_CUSTOM_ZONES | default('[]') }}
  },
  "ExtraIFaceBlackList": {{ NETBIRD_EXTRA_IFACE_BLACKLIST | default('[]') }},
  "NetworkMonitor": {
    "Enabled": {{ NETBIRD_NETWORK_MONITOR_ENABLED | default(true) }},
    "Interval": {{ NETBIRD_NETWORK_MONITOR_INTERVAL | default(30) }}
  },
  "CustomDNSAddress": "{{ NETBIRD_CUSTOM_DNS_ADDRESS | default('') }}"
}